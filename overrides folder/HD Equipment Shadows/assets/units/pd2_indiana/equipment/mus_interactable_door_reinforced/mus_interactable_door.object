<dynamic_object>
	<diesel materials="units/pd2_indiana/equipment/mus_interactable_door_reinforced/mus_interactable_door_reinforced" orientation_object="rp_mus_interactable_door" />
	<sequence_manager file="units/pd2_indiana/equipment/mus_interactable_door_reinforced/mus_interactable_door" />
	<animations>
		<animation_group name="anim_door" enabled="false">
			<object name="anim_door" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_frame" enabled="true" template="static">
			<object name="frame" />
			<object name="c_box_01" collision_type="box" />
			<object name="c_box_02" collision_type="box" />
			<object name="c_box_03" collision_type="box" />
		</body>
		<body name="body_door" enabled="true" template="animated">
			<object name="anim_door" />
			<object name="c_box_door" collision_type="box" />
		</body>
		<body name="body_hitbox_door_handle_in" enabled="true" template="static">
			<object name="frame" />
			<object name="c_hitbox_door_handle_in" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_hitbox_door_handle_out" enabled="true" template="static">
			<object name="frame" />
			<object name="c_hitbox_door_handle_out" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_glass" enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_metal_door" enabled="true" material="metal" />
		<decal_mesh name="dm_metal_frame" enabled="true" material="metal" />
	</decal_surfaces>
	<graphics>

		<graphic_group name="grp_icon" enabled="false" >
			
			<lod_object name="lod_saw_icon">
					<object name="g_icon_saw" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_saw_lod1" 	enabled="true"  lod="1" />
			</lod_object>

			<lod_object name="lod_lockpick_icon">
					<object name="g_icon_lockpick" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lockpick_lod1" 	enabled="true"  lod="1" />
			</lod_object>

		</graphic_group>

		<graphic_group name="grp_doors" enabled="true" culling_object="co_door">
			<object name="g_door" enabled="true" />
			<object name="g_door_expload" enabled="false" />
		</graphic_group>

	</graphics>
</dynamic_object>
