<dynamic_object>
	<diesel materials="units/pd2_indiana/equipment/mus_interactable_win_small/mus_interactable_win_small" orientation_object="rp_mus_interactable_win_small" />
	<sequence_manager file="units/pd2_indiana/equipment/mus_interactable_win_small/mus_interactable_win_small" />
	<animations>
		<animation_group name="anim" enabled="false">
			<object name="anim_door" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_frame" enabled="true" template="raycoll_static">
			<object name="rp_mus_interactable_win_small" />
			<object name="c_box_01" collision_type="box"  />
			<object name="c_box_02" collision_type="box"  />
			<object name="c_box_03" collision_type="box"  />
			<object name="c_box_04" collision_type="box"  />
		</body>
		<body name="body_window" enabled="true" template="window_animated">
			<object name="anim_door" />
			<object name="c_box_glass" collision_type="box"  />
		</body>
		<body name="body_blocker_player" enabled="true" template="mover_blocker">
			<object name="rp_mus_interactable_win_small" />
			<object name="c_box_player_blocker" collision_type="box"  />
		</body>
	</bodies>
	<decal_surfaces default_material="thin_layer">
		<decal_mesh name="dm_wood" enabled="true" material="wood" />
	</decal_surfaces>
	<graphics>
		<graphic_group name="grp_win" enabled="true" culling_object="co_cull">
			<object name="g_glass"	enabled="true" shadow_caster="false" />
			<object name="g_g"		enabled="true" shadow_caster="false" />
			<object name="s_s"		enabled="true" shadow_caster="true" />
		</graphic_group>

		<graphic_group name="grp_icon" enabled="true" >
			<lod_object name="lod_lockpick_icon">
					<object name="g_icon_lockpick" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lockpick_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
	</graphics>
</dynamic_object>
