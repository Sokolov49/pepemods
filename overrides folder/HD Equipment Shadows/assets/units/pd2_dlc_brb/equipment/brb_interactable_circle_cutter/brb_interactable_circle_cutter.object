<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_dlc_brb/equipment/brb_interactable_circle_cutter/brb_interactable_circle_cutter" orientation_object="rp_brb_interactable_circle_cutter" />
	<sequence_manager file="units/pd2_dlc_brb/equipment/brb_interactable_circle_cutter/brb_interactable_circle_cutter" />
	
	<animations>
		<animation_group name="anim_arm" enabled="false" looping="true">
			<object name="a_pivot" />
		</animation_group>
	</animations>
	
	<bodies>
		<body name="trunk_body" enabled="true" template="editor">
			<object name="rp_brb_interactable_circle_cutter" />
			<object name="g_g" collision_type="box" />
		</body>
	</bodies>

	<effects>
		<effect_spawner name="es_sparks" enabled="false" object="e_sparks" effect="effects/payday2/particles/equipment/welding_effect" />
		<effect_spawner name="es_smoke" enabled="false" object="e_effect_01" effect="effects/payday2/environment/drill_jammed" />
    </effects>
	
	<graphics>
		<object name="g_g" 					enabled="false" />
		<object name="g_light_on" 			enabled="false" />
		<object name="g_light_jammed" 		enabled="false" />
		<object name="g_rotate"				enabled="false" />
		<graphic_group name="grp_contour" 	enabled="true">
			<lod_object name="lod_contour">
				<object name="g_contour_lod0" enabled="true"  max_distance="1000" />
				<object name="g_contour_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
		<graphic_group name="grp_contour_jam" 	enabled="false">
			<object name="g_contour_jam_rotate" enabled="true" />
			<object name="g_contour_jam" enabled="true" />
		</graphic_group>
	</graphics>

<lights>
	<light name="ls_light_jammed" 	enabled="false" editable="true" type="omni|specular" multiplier="0.1" far_range="350" falloff="4" />
	<light name="ls_light" 			enabled="false" editable="true" type="omni|specular" multiplier="0.1" far_range="350" falloff="4" />
</lights>
</dynamic_object>
