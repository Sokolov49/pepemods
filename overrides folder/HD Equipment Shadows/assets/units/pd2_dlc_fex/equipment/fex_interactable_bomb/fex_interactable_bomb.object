<dynamic_object>
	<diesel materials="units/pd2_dlc_fex/equipment/fex_interactable_bomb/fex_interactable_bomb" orientation_object="rp_fex_interactable_bomb" />
	<sequence_manager file="units/pd2_dlc_fex/equipment/fex_interactable_bomb/fex_interactable_bomb" />
	<animations>
		<animation_group name="anim_timer" enabled="false">
			<object name="anim_timer" />
