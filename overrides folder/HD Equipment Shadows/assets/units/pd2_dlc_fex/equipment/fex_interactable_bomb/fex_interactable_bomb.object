			<object name="rp_fex_interactable_bomb" />
			<object name="c_box" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_static" enabled="false" template="static">
			<object name="rp_fex_interactable_bomb" />
			<object name="c_box" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<graphics>
		<graphic_group name="grp_bomb" enabled="false" >
			<object name="g_bomb" enabled="true" />
			<object name="g_mullplan" enabled="true" />
			<object name="g_timer_top" enabled="true" />
		</graphic_group>
		<graphic_group name="grp_outline" enabled="true">
			<lod_object name="lod_outline">
				<object name="g_outline" enabled="true"  max_distance="500" />
				<object name="g_outline_lod" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>
</dynamic_object>
