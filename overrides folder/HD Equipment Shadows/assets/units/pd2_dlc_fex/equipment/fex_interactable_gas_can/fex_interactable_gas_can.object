<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_dlc_fex/equipment/fex_interactable_gas_can/fex_interactable_gas_can" orientation_object="rp_thermite" />
	<sequence_manager file="units/pd2_dlc_fex/equipment/fex_interactable_gas_can/fex_interactable_gas_can" />

	<bodies>
		<body name="static_body" enabled="true" template="animated">
			<object name="rp_thermite" />
			<object name="g_thermite" collision_type="box" padding="-2"/>
		</body>
		<body name="body_hitbox" enabled="false" template="animated">
			<object name="rp_thermite"/>
			<object name="c_thermite" collision_type="box" />
		</body>
	</bodies>
	
	<graphics>
			<graphic_group name="grp_showitself" enabled="false">
				<object name="g_thermite" enabled="true" />
			</graphic_group>
				<graphic_group name="grp_outline" enabled="false" >
			<lod_object name="lod_outline">
				<object name="g_outline" enabled="true"  max_distance="500" />
				<object name="g_outline_lod" enabled="false" />
			</lod_object>
		</graphic_group>
	</graphics>

</dynamic_object> 