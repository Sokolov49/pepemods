<dynamic_object>
	<diesel materials="units/pd2_dlc_chas/equipment/chas_interactable_alarmbox/chas_interactable_alarmbox" orientation_object="rp_chas_interactable_alarmbox" />
	<sequence_manager file="units/pd2_dlc_chas/equipment/chas_interactable_alarmbox/chas_interactable_alarmbox"/>
	
    <animations>
        <animation_group name="anim_open_door" enabled="false">
            <object name="anim_hinge" />
        </animation_group>
    </animations>

	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="rp_chas_interactable_alarmbox" />
			<object name="c_box_000" collision_type="box" padding="-2.5" />
		</body>

			<body name="body_pcb" enabled="false" template="static">
			<object name="rp_chas_interactable_alarmbox" />
			<object name="c_pcb" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<decal_surfaces default_material="sheet_metal">
		<decal_mesh name="dm_concrete" enabled="true" material="concrete" />
		<decal_mesh name="dm_metal" enabled="true" material="metal" />
		<decal_mesh name="dm_sheet_metal" enabled="true" material="sheet_metal" />
	</decal_surfaces>
	<graphics>
		<object name="g_door" enabled="true" />
		<object name="g_pcb" enabled="true" />
		<object name="g_pcb_broken" enabled="false" />
		<object name="g_pcb_screen_lit" enabled="true" />
		<object name="g_b" enabled="true" />
		<object name="g_g" enabled="true" />
		<object name="s_door" enabled="true" shadow_caster="true" />
		<object name="s_s" enabled="true" shadow_caster="true" />
		<object name="c_pcb" enabled="false" shadow_caster="false" />
		

			<graphic_group name="grp_outline" enabled="true">
    			<lod_object name="lod_outline">
        			<object name="g_outline"         enabled="true" max_distance="1000"/>
        			<object name="g_outline_lod"     enabled="true"/>
    			</lod_object>
			</graphic_group>

	</graphics>

</dynamic_object>
