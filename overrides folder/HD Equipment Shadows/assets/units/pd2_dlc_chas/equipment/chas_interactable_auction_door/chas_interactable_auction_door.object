<dynamic_object>
	<diesel orientation_object="rp_chas_interactable_auction_door" materials="units/pd2_dlc_chas/equipment/chas_interactable_auction_door/chas_interactable_auction_door" />
	<sequence_manager file="units/pd2_dlc_chas/equipment/chas_interactable_auction_door/chas_interactable_auction_door" />

	<animations>
		<animation_group name="anim_open_door" enabled="false">
			<object name="anim_hinge_1" />
			<object name="anim_hinge_2" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_frame" enabled="true" template="static" remove_ray_type="walk" >
			<object name="rp_chas_interactable_auction_door"/>
			<object name="c_frame" collision_type="mesh_mopp" padding="-2.5"  />
		</body>
		<body name="body_door" enabled="true" template="animated">
			<object name="g_door_0" />
		</body>
		<body name="body_door2" enabled="true" template="animated">
			<object name="g_door_1" />
		</body>

		<body name="body_hitbox_door_handle_in" enabled="true" template="static" >
			<object name="c_hitbox_door_handle_in" collision_type="box" padding="-2.5" />
		</body>

		<body name="body_hitbox_door_handle_out" enabled="true" template="static" >
			<object name="c_hitbox_door_handle_out" collision_type="box" padding="-2.5" />
		</body>

		<body name="body_hitbox_door_1" enabled="true" template="animated" >
			<object name="c_box_door_1" collision_type="box" padding="-2.5" />
		</body>

		<body name="body_hitbox_door__2" enabled="true" template="animated" >
			<object name="c_box_door_2" collision_type="box" padding="-2.5" />
		</body>
	</bodies>

	<decal_surfaces default_material="wood" static="false">
		<decal_mesh name="dm_wood_frame" 		enabled="true" 	material="wood" />

		<decal_mesh name="dm_wood_hinge_1" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_hinge_2" 		enabled="true" 	material="wood" />

		<decal_mesh name="dm_metal_hinge_1" 		enabled="true" 	material="metal" />
		<decal_mesh name="dm_metal_hinge_2" 		enabled="true" 	material="metal" />
	</decal_surfaces>

	<graphics>

		<object name="g_door_0" enabled="true"/>
		<object name="g_door_1" enabled="true"/>
		<object name="g_door_breached1" enabled="false" />
		<object name="g_door_breached2" enabled="false" />
		<object name="g_splinters1" enabled="false" />
		<object name="g_splinters2" enabled="false" />
		<object name="g_door_breached1" enabled="false" />
		<object name="g_door_breached2" enabled="false" />
		<object name="g_splinters1" enabled="false" />			
		<object name="g_splinters2" enabled="false" />

		<graphic_group name="grp_icons" 	enabled="true" culling_object="g_icon1">
			<lod_object name="icon_lod">
					<object name="g_icon1" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>

			<object name="g_frame" 			enabled="true" />
			<object name="s_s_frame" 				enabled="true" 	shadow_caster="true"/>
	</graphics>
</dynamic_object>