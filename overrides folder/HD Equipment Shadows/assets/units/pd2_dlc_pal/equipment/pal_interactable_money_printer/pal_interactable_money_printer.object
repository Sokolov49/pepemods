<dynamic_object>
    <diesel orientation_object="rp_moneyprinter" materials="units/pd2_dlc_pal/equipment/pal_interactable_money_printer/pal_interactable_money_printer" />
    <sequence_manager file="units/pd2_dlc_pal/equipment/pal_interactable_money_printer/pal_interactable_money_printer" />
    <animations>
		<animation_group enabled="false" name="ag_break_lcd" >
            <object name="root" />
            <object name="joint1" />
            <object name="joint2" />
            <object name="joint3" />
            <object name="joint4" />
            <object name="joint5" />
            <object name="end" />
		</animation_group>
		<animation_group name="paper_roll_animation" enabled="false">
				<object name="root_paper" />
				<object name="joint_03" />
				<object name="joint_04" />
				<object name="joint_05" />
				<object name="joint_06" />
				<object name="joint_07" />
				<object name="anim_paper_roll" />
		</animation_group>
		<animation_group name="print" enabled="false">
				<object name="a_print" />
		</animation_group>
		<animation_group name="front_door1" enabled="false">
				<object name="a_door_1" />
		</animation_group>
		<animation_group name="front_door2" enabled="false">
				<object name="a_door_2" />
		</animation_group>
		<animation_group name="back_door1" enabled="false">
				<object name="a_door_3" />
		</animation_group>
		<animation_group name="back_door2" enabled="false">
				<object name="a_door_4" />
		</animation_group>
		<animation_group name="front_small_door1" enabled="false">
				<object name="a_small_door_1" />
		</animation_group>
		<animation_group name="front_small_door2" enabled="false">
				<object name="a_small_door_2" />
		</animation_group>
		<animation_group name="print_light" enabled="false" loop="true">
				<object name="ls_printer" />
		</animation_group>
		<animation_group name="dots" enabled="true" loop="true">
			<object name="a_dot_1" />
			<object name="a_dot_2" />
			<object name="a_dot_3" />
		</animation_group>
	</animations>
    <bodies>
    	<body name="front_door1" enabled="true" template="raycoll">
    		<object name="a_door_1" />
    		<object name="c_door_1" collision_type="box" />
    	</body>
    	<body name="front_door2" enabled="true" template="raycoll">
    		<object name="a_door_2" />
    		<object name="c_door_2" collision_type="box" />
    	</body>
    	<body name="back_door1" enabled="true" template="raycoll">
    		<object name="a_door_3" />
    		<object name="c_door_3" collision_type="box" />
    	</body>
    	<body name="back_door2" enabled="true" template="raycoll">
    		<object name="a_door_4" />
    		<object name="c_door_4" collision_type="box" />
    	</body>
    	<body name="front_small_door1" enabled="true" template="raycoll">
    		<object name="a_small_door_1" />
    		<object name="c_small_door_1" collision_type="box" />
    	</body>
    	<body name="front_small_door2" enabled="true" template="raycoll">
    		<object name="a_small_door_2" />
    		<object name="c_small_door_2" collision_type="box" />
    	</body>
        <body enabled="true" template="static" name="body0" >
            <object name="rp_moneyprinter" />
            <object collision_type="mesh_mopp" name="c_c" />
        </body>
        <body enabled="true" template="animated" name="panel" >
        	<object name="end" />
			<object collision_type="box" name="c_lcd" padding="-2"/>
			<object collision_type="convex" name="c_panel" padding="-2"/>
        </body>
    </bodies>
	<decal_surfaces default_material="sheet_metal">
        <decal_mesh enabled="true" name="dm_dm" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_print_roll" material="cloth_stuffed" />
		<decal_mesh enabled="true" name="dm_lcd" material="glass_unbreakable" />
		<decal_mesh enabled="true" name="dm_lcd_2" material="glass_unbreakable" />
		<decal_mesh enabled="true" name="dm_small_door_1" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_small_door_2" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_door_1" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_door_2" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_door_3" material="sheet_metal" />
		<decal_mesh enabled="true" name="dm_door_4" material="sheet_metal" />		
		<decal_mesh enabled="true" name="dm_panel" material="sheet_metal" />		
    </decal_surfaces>
    <graphics>
        <object enabled="true" name="g_g" />
		<object enabled="true" name="g_door_1" />
		<object enabled="true" name="g_door_2" />
		<object enabled="true" name="g_door_3" />
		<object enabled="true" name="g_door_4" />
		<object enabled="false" name="g_paperroll" />
		<object enabled="true" name="g_panel" />
		<object enabled="true" name="g_small_door_1" />
		<object enabled="true" name="g_small_door_2" />
		<object enabled="true" name="g_plane" />
		<object enabled="true" name="g_stack" />
		<object enabled="true" name="g_lcd_beam" />
		<object enabled="false" name="g_lcd_broken_beam" />
		<object enabled="true" name="g_breaker_off" />
		<object enabled="false" name="g_breaker_on" />
		<object enabled="false" name="g_breaker_off_contour" />
		<object enabled="false" name="g_ink_contour" />
		<object enabled="false" name="g_printing" />
		<object enabled="false" name="g_dot1" />
		<object enabled="false" name="g_dot2" />
		<object enabled="false" name="g_dot3" />
		<object enabled="false" name="g_panel_il" />
    </graphics>
	<lights>
		<light name="ls_screen"		enabled="false"	editable="true"	type="omni|specular"	multiplier="0.5" 	far_range="150"	falloff="4" />
		<light name="ls_printer"	enabled="false"	editable="true"	type="omni|specular"	multiplier="2" 		far_range="70" 	falloff="4" />
	</lights>
</dynamic_object>
