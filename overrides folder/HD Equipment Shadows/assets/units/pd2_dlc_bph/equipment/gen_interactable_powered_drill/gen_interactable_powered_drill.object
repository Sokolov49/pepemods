<dynamic_object>
	<diesel materials="units/pd2_dlc_bph/equipment/gen_interactable_powered_drill/gen_interactable_powered_drill" orientation_object="rp_gen_interactable_drill_small" />
	<sequence_manager file="units/pd2_dlc_bph/equipment/gen_interactable_powered_drill/gen_interactable_powered_drill" />
	<animations>
		<animation_group name="anim" enabled="false" loop="true" >
			<object name="a_anim_drill" />
			<object name="a_anim_drill_drill" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_editor" enabled="true" template="editor">
			<object name="rp_gen_interactable_drill_small" />
			<object name="c_drill" collision_type="mesh_mopp" />
		</body>
		<body name="body_static" enabled="false" template="static" collides_with_mover="false" remove_ray_type="walk vis_graph ai_vision">
			<object name="rp_gen_interactable_drill_small" />
			<object name="c_drill" collision_type="mesh_mopp" />
		</body>
	</bodies>

	<decal_surfaces default_material="steel_no_decal">
		<decal_mesh name="g_base" enabled="true" material="steel_no_decal" />
		<decal_mesh name="g_drill" enabled="true" material="steel_no_decal" />
	</decal_surfaces>


	<graphics>
		<graphic_group name="icon" enabled="true" culling_object="g_drill_icon">
			<lod_object name="drill_icon">
				<object name="g_drill_icon" 			enabled="true"  max_distance="450"  max_draw_lod="0" />
				<object name="g_drill_icon_lod1" 		enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<object name="g_base" 					enabled="false"/>
		<object name="g_drill" 					enabled="false"/>
		<object name="g_drill_drill" 			enabled="false"/>
		<object name="g_screen_off"				enabled="false"/>
		<object name="timer_gui" 				enabled="false"/>
	</graphics>
</dynamic_object>
