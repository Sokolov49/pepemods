<dynamic_object>
	<diesel materials="units/pd2_dlc_vit/equipment/vit_interactable_door_south_portico/vit_interactable_door_south_portico" orientation_object="rp_vit_interactable_door_south_portico" />
	<sequence_manager file="units/pd2_dlc_vit/equipment/vit_interactable_door_south_portico/vit_interactable_door_south_portico" />

	<animations>
		<animation_group name="anim" enabled="false">
			<object name="anim_door_right" />
			<object name="anim_door_left" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_frame" enabled="true" template="static" >
			<object name="rp_vit_interactable_door_south_portico"/>
			<object name="c_box_frame_01" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_02" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_03" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_04" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_05" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_06" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_07" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_08" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_09" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_10" collision_type="box" padding="-2.5" />
			<object name="c_box_frame_11" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_door_right" enabled="true" template="animated">
			<object name="anim_door_right" />
			<object name="c_box_door_r_01" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_02" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_03" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_04" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_05" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_06" collision_type="box" padding="-2.5" />
			<object name="c_box_door_r_07" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_door_left" enabled="true" template="animated">
			<object name="anim_door_left" />
			<object name="c_box_door_l_01" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_02" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_03" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_04" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_05" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_06" collision_type="box" padding="-2.5" />
			<object name="c_box_door_l_07" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_handle_collision_in" enabled="true" template="static">
			<object name="rp_vit_interactable_door_south_portico" />
			<object name="c_box_handle_collision_in" collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_handle_collision_out" enabled="true" template="static">
			<object name="rp_vit_interactable_door_south_portico" />
			<object name="c_box_handle_collision_out" collision_type="box" padding="-2.5"/>
		</body>
	</bodies>

	<decal_surfaces default_material="wood" static="false">
		<decal_mesh name="dm_wood_door_r" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_door_l" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_frame" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_metal_door_r" 		enabled="true" 	material="metal" />
		<decal_mesh name="dm_metal_door_l" 		enabled="true" 	material="metal" />
		<decal_mesh name="dm_metal_frame" 		enabled="true" 	material="metal" />
	</decal_surfaces>

	<graphics>

		<object name="g_frame" enabled="true" />

		<graphic_group name="grp_icons" 	enabled="true" culling_object="g_icon">
			<lod_object name="icon_lod">
					<object name="g_icon" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>

		<graphic_group name="doors" enabled="true" culling_object="co_door">
			<object name="g_door_right" 			enabled="true" />
			<object name="g_door_right_broken" 		enabled="false" />
			<object name="g_door_left" 				enabled="true" />
			<object name="g_door_left_broken" 		enabled="false" />
			<object name="g_door_left_sawed" enabled="false" />
			<object name="s_s_frame" 				enabled="true" 	shadow_caster="true"/>
			<object name="s_s_door_r" 				enabled="true" 	shadow_caster="true"/>
			<object name="s_s_door_l" 				enabled="true" 	shadow_caster="true"/>
		</graphic_group>

	</graphics>
</dynamic_object>
