<dynamic_object>
	<diesel orientation_object="rp_vit_interactable_door_wooden_white_double" materials="units/pd2_dlc_vit/equipment/vit_interactable_door_wooden_white/vit_interactable_door_wooden_white_double" />
	<sequence_manager file="units/pd2_dlc_bex/equipment/bex_interactable_door_wooden_white/bex_interactable_door_wooden_white_double" />

	<animations>
		<animation_group name="anim_open_door" enabled="false">
			<object name="hinge" />
			<object name="hinge2" />
		</animation_group>
		<animation_group name="anim_kick" enabled="false">
				<object name="door" />
				<object name="door2" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_frame" enabled="true" template="static" remove_ray_type="walk" >
			<object name="rp_vit_interactable_door_wooden_white_double"/>
			<object name="c_frame" collision_type="mesh_mopp" padding="-2.5"  />
		</body>
		<body name="body_door" enabled="true" template="animated">
			<object name="door" />
			<object name="c_box_door" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_door2" enabled="true" template="animated">
			<object name="door2" />
			<object name="c_box_door2" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_hitbox_door_handle_in" enabled="true" template="static">
			<object name="rp_vit_interactable_door_wooden_white_double" />
			<object name="c_hitbox_door_handle_in" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_hitbox_door_handle_out" enabled="true" template="static">
			<object name="rp_vit_interactable_door_wooden_white_double" />
			<object name="c_hitbox_door_handle_out" collision_type="box" padding="-2.5" />
		</body>
	</bodies>

	<decal_surfaces default_material="wood" static="false">
		<decal_mesh name="dm_wood_door" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_door2" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_frame" 		enabled="true" 	material="wood" />
	</decal_surfaces>

	<graphics>
		<graphic_group name="grp_icons" 	enabled="true" culling_object="g_icon">
			<lod_object name="icon_lod">
					<object name="g_icon" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<graphic_group name="grp_door" enabled="true" culling_object="co_door">
			<object name="g_door" enabled="true" />
		</graphic_group>
			<object name="g_door_frame" 			enabled="true" />
			<object name="g_saw_dmg" 				enabled="false" />
			<object name="g_door_explode_dmg" 		enabled="false" />
			<object name="s_s" 						enabled="true" 	shadow_caster="true"/>
			<object name="s_s2" 					enabled="true" 	shadow_caster="true"/>
			<object name="s_s_frame" 				enabled="true" 	shadow_caster="true"/>
	</graphics>
</dynamic_object>