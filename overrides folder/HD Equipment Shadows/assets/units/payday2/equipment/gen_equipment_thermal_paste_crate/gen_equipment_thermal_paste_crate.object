<dynamic_object>
    <diesel orientation_object="rp_gen_equipment_thermal_paste_crate" materials="units/payday2/equipment/gen_equipment_thermal_paste_crate/gen_equipment_thermal_paste_crate" />
	<sequence_manager file="units/payday2/equipment/gen_equipment_thermal_paste_crate/gen_equipment_thermal_paste_crate" />
	
    <animations>
        <animation_group name="anim_lock" enabled="false">
            <object enabled="true" name="a_lock" />
        </animation_group>
        <animation_group name="anim_lid" enabled="false">
            <object enabled="true" name="a_lid" />
        </animation_group>
        <animation_group name="anim_comp" enabled="false">
            <object enabled="true" name="a_pivot" />
            <object enabled="true" name="a_aligner" />
        </animation_group>
    </animations>
	
	<bodies>
		<body name="body_editor" enabled="true" template="editor">
			<object name="rp_gen_equipment_thermal_paste_crate" />
			<object name="g_lid" collision_type="box" />
			<object name="g_bottom_static" collision_type="box" />
		</body>
		<body name="static_bottom" enabled="false" template="static_no_ray" collision_class="bag" collides_with_mover="false" >
			<object name="rp_gen_equipment_thermal_paste_crate" />
			<object name="c_box_02" collision_type="box" padding="-2.5" />
		</body>
		<body name="static_top" enabled="false" template="animated_no_ray" collision_class="bag" collides_with_mover="false" >
			<object name="a_lid" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
	</bodies>	
	
    <graphics>
		<object name="g_bottom_animated" shadow_caster="true"	enabled="true"/>
		<object name="g_bottom_static" 	shadow_caster="true"	enabled="true"/>

		<object name="g_seam" shadow_caster="true"	enabled="false"/>
		<object name="g_lid"  shadow_caster="true"	enabled="true"/>
		<object name="g_lock" shadow_caster="true"	enabled="true"/>
		<object name="g_hinge" shadow_caster="true"	enabled="true"/>
    </graphics>
	
</dynamic_object>
